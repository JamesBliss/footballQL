{
  "name": "footballQL",
  "version": 2,
  "alias": "footballQL.com",
  "regions": ["bru"],
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server",
      "config": { "maxLambdaSize": "80mb"}
    }
  ],
  "routes": [
    { "src": "/.*", "dest": "index.js" }
  ],
  "env": {
    "FOOTBALL_KEY": "@football_key"
  }
}